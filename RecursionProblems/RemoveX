def removeX(string):
    if len(string)==0:
        return string
	
    
    remove = removeX(string[1:])
    if string[0]=="x":
        return ""+ remove
    else:
        return string[0]+remove
    
    return remove
    
    
    
    
    
    

# Main
string = input()
si=0
print(removeX(string))
